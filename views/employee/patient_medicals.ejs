
<%- include('../includes/emp_head.ejs') %>
</head>

<body id="page-top">
    <div id="wrapper">
        <%- include('../includes/emp_nav.ejs') %>
        <div class="d-flex flex-column" id="content-wrapper">
            <div id="content">
                <%- include('../includes/empNav.ejs', {name:employee.full_name,image:employee.imageUrl}) %>
        <div class="container-fluid">
            <h3 class="text-dark mb-4">Users Medicalhistory</h3>
            <div class="card shadow">
                <div class="card-header py-3">
                    <p class="text-primary m-0 font-weight-bold">View history</p>
                </div>
                <div class="card-body"><span>Patient&nbsp; Deteils</span>
                    <div class="table-responsive table mt-2" id="dataTable" role="grid" aria-describedby="dataTable_info">
                        <table class="table my-0" id="dataTable">
                            <thead>
                                <tr style="width: 698px;">
                                    <th>Name</th>
                                    <th>Gender</th>
                                    <th>weghit</th>
                                    <th>height</th>
                                    <th>Age</th>
                                    <th style="padding: -23px;height: 8px;"><br>Married status</th>
                                    <th>blood_group</th>
                                    <th>Phone number</th>
                                    <th>Address</th>
                                    
                                </tr>
                            </thead>
                            <tbody>
                                <tr>
                                    <td><%= patient[0].full_name %></td>
                                    <td><%= patient[0].gender %></td>
                                    <td><%= patient[0].patient_wehit %></td>
                                    <td><%= patient[0].patient_height %></td>
                                    <td><%= patient[0].age %></td>
                                    <td><%= patient[0].marriedStatus %></td>
                                    <td><%= patient[0].blood_group %></td>
                                    <td><%= patient[0].phonNumber %></td>
                                    <td><%= patient[0].district %></td>
                                </tr>
                            </tbody>
                            <tfoot>
                                <tr>
                                    <td style="padding-top: 38px;"></td>
                                    <td><strong><br><br><br></strong></td>
                                    <td><strong><br><br><br></strong></td>
                                    <td><br><br></td>
                                    <td><strong><br></strong></td>
                                    <td></td>
                                </tr>
                            </tfoot>
                        </table>
                    </div>
                </div>
                <div class="card-body"><span>Medical history</span>
                    <div class="table-responsive table mt-2" id="dataTable" role="grid" aria-describedby="dataTable_info">
                        <table class="table my-0" id="dataTable">
                            <thead>
                                <tr style="width: 698px;">
                                    <th>Name</th>
                                    <th>Doctor</th>
                                    <th>Sickness</th>
                                    <th style="padding: -23px;height: 8px;width: 143px;"><br>Insurance</th>
                                    <th style="width: 88px;">visite date</th>
                                    <th></th>
                                </tr>
                            </thead>
                            <tbody>
                                <% for (let med of medicals) { %> 
                                    <tr>
                                        <td><img class="rounded-circle mr-2" width="30" height="30" src="/img/avatars/avatar1.jpeg"><%= med.Patient_name%></td>
                                        <td><%= med.Doctor_name%></td>
                                        <td><%= med.patient_sicknesses%></td>
                                        <td style="padding: 12px;padding-top: 0;padding-bottom: 0;"><% if( med.insurance_id > 0 ){%>  <a href="patietn_insurance?insurance=<%= med.insurance_id %>"><span><%= med.insurance_name%>  <i class="fa fa-arrow-right" style="font-size: 21px;margin-top: 12px;"></i></span></a> <%} else{%> No insurance<%}%>  </td>
                                        <td><%= med.regestration_Date %></td>
                                    </tr> 
                                    <% } %> 
                               
                            </tbody>
                            <tfoot>
                                <tr>
                                    <th>Name</th>
                                    <th>Doctor</th>
                                    <th>Sickness</th>
                                    <th style="padding: -23px;height: 8px;width: 143px;"><br>Insurance</th>
                                    <th style="width: 88px;">visite date</th>
                                    <th></th>
                                </tr>
                            </tfoot>
                        </table>
                    </div>
                    <div class="row">
                        <div class="col-md-6 align-self-center">
                            <p id="dataTable_info" class="dataTables_info" role="status" aria-live="polite">Showing <% if(start == 0 ){ %> 1 <%} else {%> <%=start%> <% } %> to <%= start + 5 %> of <%=totalItem%></p>
                        </div>
                        <div class="col-md-6">
                            <nav class="d-lg-flex justify-content-lg-end dataTables_paginate paging_simple_numbers">
                                <%- include('../includes/pagination.ejs', {curentpage:curentpage,hasnextpage:hasnextpage,haspreviouspage:haspreviouspage,nextpage:nextpage,previouspage:previouspage,lastpage:lastpage }) %>

                            </nav>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
    


    <footer class="bg-white sticky-footer">
        <div class="container my-auto">
            <div class="text-center my-auto copyright"><span>Copyright © Brand 2021</span></div>
        </div>
    </footer>
    </div><a class="border rounded d-inline scroll-to-top" href="#page-top"><i class="fas fa-angle-up"></i></a></div>
    <div class="modal text-center" role="dialog" tabindex="-1" id="modal">
